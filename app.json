{
  "expo": {
    "name": "SkateSpot",
    "slug": "SkateSpot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "An app for users to discover obstacles and or skate parks around them. Also includes a minigame that prompts the user to complete a trick at random. ",
    "githubUrl": "https://github.com/brodrickgeorge/SkateSpot"
  }
}
